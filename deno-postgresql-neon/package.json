{
  "name": "netlify-postgresql-neon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn dev_db & next dev & yarn deno_run && kill $!",
    "dev_db": "docker-compose run --rm --service-ports db  &> /dev/null",
    "deno_run": "deno run --allow-env --allow-net --allow-read=.env,.env.defaults,out  deno/server.ts",
    "build": "next build && next export"
  },
  "dependencies": {
    "@subzerocloud/nodejs": "^0.1.9",
    "itty-router": "^2.6.6"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "18.11.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/yargs": "^17.0.13",
    "autoprefixer": "^10.4.12",
    "next": "12.3.1",
    "postcss": "^8.4.18",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.4"
  }
}
